{
  "name": "mars",
  "version": "0.20.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start:dev": "ng serve",
    "start": "node server/app.js",
    "test": "karma start ./karma.conf.js",
    "build": "ng build --prod --aot --build-optimizer --stats-json --named-chunks",
    "dev": "webpack --mode development ./src/index.js --output ./dist/main.js",
    "deploy:dev": "git add . && npm version minor --force -m \"v.%s\"",
    "deploy:alpha": "heroku git:remote -a marsbars04 && git push heroku master",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "heroku-postbuild": "ng build --prod --aot --build-optimizer --stats-json --named-chunks",
    "postversion": "git push origin master && git push --tags && heroku git:remote -a marsbars04 && git push heroku master -f"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@types/jquery": "^3.3.22",
    "angular-cli-builders": "^2.1.2",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.4",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "heroku-ssl-redirect": "0.0.4",
    "http": "0.0.0",
    "jquery": "^3.3.1",
    "path": "^0.12.7",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^2.5.4",
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.36",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "webpack-cli": "^3.1.2"
  }
}
